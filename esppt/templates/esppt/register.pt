<html metal:use-macro="load: ../apptemplate.pt">
    <div metal:fill-slot="content">
        <script type='text/javascript'>
            $(document).ready(function () {
                $("#btn_save").click(function () {
                    $.ajax({
                        type: "POST",
                        url: "/reg/all",
                        data: $("#myform").serialize(),
                        success: function (html) {
                            if (html.success == true) {
                                $("#errors").css('display', 'inline', 'important');
                                $("#errors").html("<span class='glyphicon glyphicon-ok'></span> " + html.msg);
                            } else {
                                $("#errors").css('display', 'inline', 'important');
                                $("#errors").html("<span class='glyphicon glyphicon-remove'></span> " + html.msg);
                            }
                        },
                        beforeSend: function () {
                            $("#errors").css('display', 'inline', 'important');
                            $("#errors").html("<img src=\'${request.static_url('esppt:static/images/ajax-loader.gif')}\' /> Loading...")
                        }
                    });
                    return false;
                });
            
                $("#tgl_bayar").datepicker({dateFormat:'dd-mm-yy'});
            });
        </script>
        <div class="panel-body">
            
            <!--div class="page-header">
                <h1>Registrasi e-SPPT</h1>
            </div-->
            <!--h3>Registrasi e-SPPT</h3-->
            <img src="${request.static_url('esppt:static/images/logo.png')}" style="height:80px;" />
            
            <div class="row">
                <form method="post" accept-charset="utf-8" id="myform" class="form-horizontal" enctype="multipart/form-data">
                    <div class="col-md-6">
                        <div class="panel panel-primary">
                            <div class="panel-heading">
                                <h3 class="panel-title">DENTITAS PEMILIK/PEMANFAAT/PENGELOLA OBJEK PAJAK  PBB</h3>
                            </div>
                            <div class="panel-body">
                                <!--div class="form-group">
                                    <div class="col-xs-8 col-sm-offset-4">
                                        <h3>IDENTITAS</h3>
                                    </div>
                                </div--->
                                
                                <div class="form-group">
                                    <label for="kode" class="control-label col-xs-4">NIK</label>
                                    <div class="col-xs-8">
                                        <input class="form-control"  type="text" name="kode" id="kode" value="${rows and rows.kode}">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="nama" class="control-label col-xs-4">Nama</label>
                                    <div class="col-xs-8">
                                        <input class="form-control"  type="text" name="nama" id="nama" value="${rows and rows.nama}">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="alamat1" class="control-label col-xs-4">Alamat</label>
                                    <div class="col-xs-8">
                                        <input class="form-control"  type="text" name="alamat1" id="alamat1" value="${rows and rows.alamat1}">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="alamat2" class="control-label col-xs-4"></label>
                                    <div class="col-xs-8">
                                        <input class="form-control"  type="text" name="alamat2" id="alamat2" value="${rows and rows.alamat2}">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="kelurahan" class="control-label col-xs-4">Kelurahan</label>
                                    <div class="col-xs-8">
                                        <input class="form-control"  type="text" name="kelurahan" id="kelurahan" value="${rows and rows.kelurahan}">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="kecamatan" class="control-label col-xs-4">Kecamatan</label>
                                    <div class="col-xs-8">
                                        <input class="form-control"  type="text" name="kecamatan" id="kecamatan" value="${rows and rows.kecamatan}">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="kabupaten" class="control-label col-xs-4">Kabupaten/Kota</label>
                                    <div class="col-xs-8">
                                        <input class="form-control" type="text" name="kabupaten" id="kabupaten" value="${rows and rows.kabupaten}">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="propinsi" class="control-label col-xs-4">Propinsi</label>
                                    <div class="col-xs-8">
                                        <input class="form-control" type="text" name="propinsi" id="propinsi" value="${rows and rows.propinsi}">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="no_hp" class="control-label col-xs-4">No. Handphone</label>
                                    <div class="col-xs-8">
                                        <input class="form-control" type="text" name="no_hp" id="no_hp" value="${rows and rows.no_hp}">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="email" class="control-label col-xs-4">e-mail</label>
                                    <div class="col-xs-8">
                                        <input class="form-control" type="text" name="email" id="email" value="${rows and rows.email}">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="password1" class="control-label col-xs-4">Password</label>
                                    <div class="col-xs-8">
                                        <input class="form-control"  type="password" name="password1" id="password1" value="${rows and rows.password}" placeholder="Password e-SPPT PBB dengan minimal 6 karakter">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="password2" class="control-label col-xs-4">Konfirmasi</label>
                                    <div class="col-xs-8">
                                        <input class="form-control"  type="password" name="password2" id="password2" value="${rows and rows.password}" placeholder="Masukan kembali password e-SPPT PBB">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="password2" class="control-label col-xs-4">&nbsp;</label>
                                    <div class="col-xs-8">
                                        <label for="password2" class="control-label text-danger">** Data di Isi sesuai dengan yang tertera pada KTP</label>
                                    </div>
                                    
                                </div>
                            </div>
                        </div>
                    </div>
                        
                        
                    <div class="col-md-6">
                        <div class="panel panel-primary">
                            <div class="panel-heading">
                                <h3 class="panel-title">OBJEK PAJAK</h3>
                            </div>
                            <div class="panel-body">
  
                                <!--div class="form-group">
                                    <div class="col-xs-8 col-sm-offset-4">
                                        <h3>OBJEK PAJAK</h3>
                                    </div>
                                </div-->
                            
                                <div class="form-group">
                                    <label for="nop" class="control-label col-xs-4">NOP</label>
                                    <div class="col-xs-8">
                                        <input class="form-control" type="text" name="nop" id="nop" value="${rows and rows.nop}" placeholder="Isi sesuai dengan penulisan pada SPPT PBB">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="nm_wp" class="control-label col-xs-4">Nama Wajib Pajak</label>
                                    <div class="col-xs-8">
                                        <input class="form-control"  type="text" name="nm_wp" id="nm_wp" value="${rows and rows.nm_wp}" placeholder="Isi sesuai dengan penulisan pada SPPT PBB">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="tgl_bayar" class="control-label col-xs-4">Tgl. Pembayaran PBB Terakhir </label>
                                    <div class="col-xs-8" style="width:110px !important;">
                                        <input class="form-control"  type="text" name="tgl_bayar" id="tgl_bayar" value="${rows and rows.tgl_bayar}">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="tahun" class="control-label col-xs-4">Tahun SPPT</label>
                                    <div class="col-xs-8" style="width:80px !important;">
                                        <input class="form-control"  type="text" name="tahun" id="tahun" value="${rows and rows.tahun}">
                                    </div>
                                </div>
                                
                                <div class="form-group">
                                    <label class="control-label col-xs-4">&nbsp;</label>
                                    <div class="col-xs-8">
                                        <button type="button" class="btn btn-primary btn-lg" id="btn_save" name="btn_save">Register</button>
                                        <button type="reset" class="btn btn-info btn-lg" id="btn_cancel">Batal</button>
                                    </div>
                                </div>
                            
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</html>